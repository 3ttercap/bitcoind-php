{
    "name": "nbobtc/bitcoind-php",
    "homepage": "https://github.com/nbobtc/bitcoind-php",
    "type": "library",
    "description": "PHP Wrapper for a bitcoind daemon",
    "keywords": ["bitcoin", "bitcoind", "bitcoins"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes"
        }
    ],
    "support": {
        "issues": "https://github.com/nbobtc/bitcoind-php/issues",
        "wiki": "https://github.com/nbobtc/bitcoind-php/wiki",
        "source": "https://github.com/nbobtc/bitcoind-php"
    },
    "autoload": {
        "psr-0": { "Nbobtc": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "1.x-dev": "1.3.x-dev",
            "2.x-dev": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "lib-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phing/phing": "*",
        "sami/sami": "*",
        "symfony/yaml": ">= 2.1.0 <= 2.6.5",
        "nikic/php-parser": ">= 1.0.0 <= 1.1.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}
